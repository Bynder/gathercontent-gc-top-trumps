{
   "parser": "babel-eslint",
   "extends": ["plugin:jsx-a11y/recommended"],
   "globals": {
      "window": true,
      "document": true,
      "FormData": true,
      "Node": true,
      "$": true,
      "localStorage": true,
      "navigator": true
   },
   "plugins": ["jsx-a11y"],
   "rules": {
      "jsx-a11y/rule-name": "warn"
   }
}
